{
  "name": "react-template",
  "version": "0.1.0",
  "description": "A starting point for React/Flux apps with web services, authentication, routing and unit testing.",
  "main": "index.jsx",
  "scripts": {
    "eslint": "eslint -c eslint.config src --ext .js,.jsx",
    "eslint-tests": "eslint -c eslint-tests.config test --ext .js,.jsx",
    "compile": "babel src --out-dir build/js",
    "test": "COVERAGE=true ARTIFACTS_DIR=./build/reports jenkins-mocha test/**/*.js*",
    "check": "istanbul check-coverage",
    "devtest": "ARTIFACTS_DIR=./build/reports jenkins-mocha --no-coverage test/**/*.js*",
    "start": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmascenik/react-template.git"
  },
  "keywords": [
    "react",
    "flux"
  ],
  "author": "Dan Mascenik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmascenik/react-template/issues"
  },
  "homepage": "https://github.com/dmascenik/react-template#readme",
  "dependencies": {
    "material-ui": "^0.14.1",
    "radium": "^0.14.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-tap-event-plugin": "^0.2.1"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.3",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-react": "^3.13.1",
    "eslint-plugin-standard": "^1.3.1",
    "express": "^4.13.3",
    "istanbul": "^0.4.1",
    "jenkins-mocha": "^2.5.0",
    "jsx-test": "^0.8.2",
    "nodemon": "^1.8.1",
    "react-hot-loader": "^1.3.0",
    "serve-static": "^1.10.0",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1"
  }
}
